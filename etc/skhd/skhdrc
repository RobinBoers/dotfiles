lalt - return : open -na Alacritty.app
lalt - q : yabai -m window --close

f11 : yabai -m window --toggle zoom-fullscreen
lalt - f : yabai -m window --toggle float

ctrl + alt - r : skhd --restart-service && yabai --restart-service
ctrl + alt - q : yabai --stop-service

cmd - 1 : /Users/axcelott/bin/beta/pass-gui
cmd - 2 : /Users/axcelott/bin/beta/2fa-gui

cmd - tab : yabai -m space --focus recent
lalt - tab : yabai -m space --focus recent

cmd + ctrl - left : yabai -m space --focus prev
cmd + ctrl - right : yabai -m space --focus next

lalt - down : yabai -m window --focus south
lalt - left : yabai -m window --focus west
lalt - right : yabai -m window --focus east
lalt - up : yabai -m window --focus north

lalt - 1 : yabai -m space --focus 1
lalt - 2 : yabai -m space --focus 2
lalt - 3 : yabai -m space --focus 3
lalt - 4 : yabai -m space --focus 4
lalt - 5 : yabai -m space --focus 5
lalt - 6 : yabai -m space --focus 6
lalt - 7 : yabai -m space --focus 7
lalt - 8 : yabai -m space --focus 8
lalt - 9 : yabai -m space --focus 9
lalt - 0 : yabai -m space --focus 10

lalt + ctrl - down : yabai -m window --resize bottom:0:30 --resize top:0:30;
lalt + ctrl - left : yabai -m window --resize left:-30:0 --resize right:-30:0
lalt + ctrl - right : yabai -m window --resize right:30:0 --resize left:30:0
lalt + ctrl - up : yabai -m window --resize top:0:-30 --resize bottom:0:-30;

lalt + shift - down : yabai -m window --warp south
lalt + shift - left : yabai -m window --warp west
lalt + shift - right : yabai -m window --warp east
lalt + shift - up : yabai -m window --warp north

lalt + shift - 1 : yabai -m window --space 1
lalt + shift - 2 : yabai -m window --space 2
lalt + shift - 3 : yabai -m window --space 3
lalt + shift - 4 : yabai -m window --space 4
lalt + shift - 5 : yabai -m window --space 5
lalt + shift - 6 : yabai -m window --space 6
lalt + shift - 7 : yabai -m window --space 7
lalt + shift - 8 : yabai -m window --space 8
lalt + shift - 9 : yabai -m window --space 9
lalt + shift - 0 : yabai -m window --space 10
