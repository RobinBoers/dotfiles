printf "\e]P8%s\e]P9%s\e]PA%s\e]PB%s\e]PC%s\e]PD%s\e]PE%s\e]PF%s" \
    "${color09:?}" "${color10:?}" "${color11:?}" "${color12:?}" \
    "${color13:?}" "${color14:?}" "${color15:?}" "${color16:?}"

