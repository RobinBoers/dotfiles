color02="C2185B"
color03="C2185B"
color04="C2185B"
color05="673AB7"
color06="C2185B"
color07="00BCD4"
color08="9E9E9E"
color09="000000"
color10="C2185B"
color11="C2185B"
color12="C2185B"
color13="673AB7"
color14="C2185B"
color15="00BCD4"
color16="9E9E9E"

